{
  "name": "training-diary",
  "version": "0.0.1",
  "description": "training diary",
  "repository": "https://github.com/maksim-lunochkin/training-diary",
  "main": "./server/index.js",
  "scripts": {
    "server": "DEBUG=*,-nodemon*,-express*,-send,-compression nodemon -e js,json,html -w static -w server server/index.js",
    "clean": "rimraf ./static/dist/*.js",
    "watch": "npm run clean && webpack --config config/webpack.dev.js --watch --progress --profile --bail",
    "build": "npm run clean && webpack --config config/webpack.production.js",
    "lint-server": "(eslint server) || true",
    "lint-client": "(eslint client) || true",
    "lint": "npm run lint-client && npm run lint-server",
    "start": "npm run server & npm run watch"
  },
  "author": "@maksim-lunochkin",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "debug": "^2.6.0",
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.12.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "nodemon": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-merge": "^2.3.1"
  }
}
